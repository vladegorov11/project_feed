<table class="table table-striped">
  <thead>
    <tr class="">
      <td>Имя  </td>
      <td>Фамилия </td>
      <td>Email </td>
    </tr>
  </thead>
  <tbody>
    <%- @user.each do |user|%>
      <tr class="">
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <% if user.create_feed == false %>
          <td><%= link_to 'Добавить создание новостей ', admin_create_content_for_user_path(user) %></td>
        <% else %>
          <td><%= link_to 'Убрать  создание новостей', admin_create_content_for_user_path(user) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
