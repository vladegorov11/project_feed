<h4 style=" text-align: center;"><%= 'Новости в категории:  ' + @category.name%></h4>	
<div class="row">
  <div class="col-md-8 feeds-wrapper">
    <section id='feeds'>
      <%= render partial: 'feeds/feed', collection: @feeds%>
    </section>
  </div>
  <div class="col-md-4 top-feed-wrap">
    <%= render 'shared/top_feeds'%>
  </div>
</div>
<%= will_paginate @feeds %>
